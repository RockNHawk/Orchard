@using MnLab.PdfVisualDesign.Drivers
@using MnLab.PdfVisualDesign.Binding
@using MnLab.PdfVisualDesign.Binding.Elements

@model TemplateSupportPartViewModel
@{
    var model = Model;
}
@{

    var contentId = model.ContentItem?.Id;
    @contentId

}
Edit for Parts.TempalteSupportPart

@using (Script.Foot())
{
    <script src="~/Modules/MnLab.PdfVisualDesign/Scripts/TempalteSupport/index.js"></script>
    if (contentId != null && contentId > 0)
    {
        <script>
    var url = '@(base.WorkContext.CurrentSite.BaseUrl)/Admin/MnLab.PdfVisualDesign/GetContentValueMap/@(contentId)';
    syncContentPart(url,null,$("form[action='/Admin/Contents/Edit/@contentId']"));
        </script>
    }

}